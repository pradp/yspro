	<!-- 定时任务：定时重新生成静态页面 -->
	<bean id="genStaticHtmlTrigger" class="org.springframework.scheduling.quartz.CronTriggerBean">
		<property name="jobDetail">
			<bean class="org.springframework.scheduling.quartz.JobDetailBean">
				<property name="jobClass" value="com.imchooser.infoms.util.GenAllStaticHtml" />
				<property name="jobDataAsMap">
					<map>
						<entry key="serverUrl" value="http://127.0.0.1:7080/SportsScore/" />
					</map>
				</property>
			</bean>
		</property>
		<!-- fire at 00:01 every day -->
		<property name="cronExpression" value="0 1 0 * * ?" />
	</bean>
	<!-- 定时器 -->
	<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean" autowire="no">
		<property name="triggers">
			<list>
				<ref bean="genStaticHtmlTrigger" />
			</list>
		</property>
	</bean>
